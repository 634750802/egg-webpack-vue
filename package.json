{
  "name": "egg-webpack-vue",
  "version": "0.3.0",
  "description": "egg webpack building solution for vue",
  "eggPlugin": {
    "name": "webpackvue",
    "env": [
      "local"
    ]
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin"
  ],
  "dependencies": {
    "autoprefixer": "^6.7.7",
    "easywebpack": "^0.3.0",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^11.3.4",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.3.2"
  },
  "devDependencies": {
    "autod": "^2.7.1",
    "egg": "^1.0.0",
    "egg-bin": "^3.1.0",
    "egg-ci": "^1.5.0",
    "egg-mock": "^3.2.0",
    "eslint": "^3.18.0",
    "eslint-config-egg": "^3.2.0",
    "supertest": "^3.0.0",
    "webstorm-disable-index": "^1.1.2"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "start": "node index.js",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "files": [
    "index.js",
    "app.js",
    "agent.js",
    "config",
    "app",
    "lib",
    "builder"
  ],
  "ci": {
    "version": "6, 7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hubcarl/egg-webpack-vue.git"
  },
  "bugs": {
    "url": "https://github.com/hubcarl/egg-webpack-vue/issues"
  },
  "homepage": "https://github.com/hubcarl/egg-webpack-vue#readme",
  "author": "hubcarl@126.com",
  "license": "MIT"
}
